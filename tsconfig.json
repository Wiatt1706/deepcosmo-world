{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/comment-editor/index.jsx", "app/land/[id]/page.jsx", "components/utils/icons.jsx", "components/layout/navbar.jsx", "types/database.types.ts", "components/World/environment/Lights.jsx", "components/World/element/Player.jsx", "components/land/home-land.jsx", "app/page.tsx", "components/assembly/comment-editor/index.jsx", "components/utils/helpers/index.js", "components/canvas/game.tsx", "components/hook/useKeyPress.jsx"],
  "exclude": ["node_modules"]
}
